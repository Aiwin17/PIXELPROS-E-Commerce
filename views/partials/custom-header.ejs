<!DOCTYPE html>
<html class="no-js" lang="en">
  <!-- Mirrored from wp.alithemes.com/html/evara/evara-frontend/shop-cart.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 01 Aug 2021 15:26:06 GMT -->
  <head>
    <meta charset="utf-8" />
    <title>mycamz</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta property="og:title" content="" />
    <meta property="og:type" content="" />
    <meta property="og:url" content="" />
    <meta property="og:image" content="" />
    <!-- Favicon -->
    <!-- Template CSS -->
    <link rel="stylesheet" href="/customassets/css/main.css" />

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
  </head>

  <body>
    <header class="header-area header-style-1 header-height-2">
      <div class="header-top header-top-ptb-1 d-none d-lg-block">
        <div class="container">
          <div class="row align-items-center">
            <div class="col-xl-3 col-lg-4">
              <div class="header-info">
                <ul>
                </ul>
              </div>
            </div>
            <div class="col-xl-6 col-lg-4">
              <div class="text-center">
                <div id="news-flash" class="d-inline-block">
                  <ul>
                  </ul>
                </div>
              </div>
            </div>
            <div class="col-xl-3 col-lg-4">
              <div class="header-info header-info-right">
                <ul>
                  <% if(user){ %>
                  <li>
                    <i class="fi-rs-user"></i
                    ><a href="/user-profile"><%=user.username%></a> <% } else {
                    %>
                  </li>

                  <li><i class="fi-rs-user"></i><a href="/login">LogIn</a></li>
                  <% } %>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="header-middle header-middle-ptb-1 d-none d-lg-block">
        <div class="container">
          <div class="header-wrap">
            <div class="logo logo-width-1">
              <a href="/"><img src="" alt="logo" /></a>
            </div>
            <div class="header-right">
              <div class="search-style-2">
                <form action="/search" method="post">
                  <input
                    type="text"
                    name="search"
                    value=""
                    placeholder="Search for items..."
                  />
                </form>
              </div>
              <div class="header-action-right">
                <div class="header-action-2">
                  <% if(user){ %>
                  <div class="header-action-icon-2">
                    <a href="/wishlist">
                      <img
                        class="svgInject"
                        alt="Evara"
                        src="customassets/imgs/theme/icons/icon-heart.svg"
                      />
                      <span id="wishCount" class="pro-count blue"
                        ><%= wishlistCount %></span
                      >
                    </a>
                  </div>
                  <% } else { %>
                  <div class="header-action-icon-2">
                    <a href="/login">
                      <img
                        class="svgInject"
                        alt="Evara"
                        src="customassets/imgs/theme/icons/icon-heart.svg"
                      />
                    </a>
                  </div>
                  <% } %>
                  <div class="header-action-icon-2" id="addToCart">
                    <% if(user){ %>
                    <a class="mini-cart-icon" href="/cart">
                      <img
                        alt="Evara"
                        src="customassets/imgs/theme/icons/icon-cart.svg"
                      />
                      <span id="cartCount" class="pro-count blue"
                        ><%=cartCount%></span
                      >
                    </a>
                    <% } else { %>
                    <a class="mini-cart-icon" href="/login">
                      <img
                        alt="Evara"
                        src="customassets/imgs/theme/icons/icon-cart.svg"
                      />
                    </a>
                    <% } %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="header-bottom header-bottom-bg-color sticky-bar">
        <div class="container">
          <div class="header-wrap header-space-between position-relative">
            <div class="logo logo-width-1 d-block d-lg-none">
              <a><img src="customassets/imgs/theme/logo.svg" alt="logo" /></a>
            </div>
            <div class="header-nav d-none d-lg-flex">
              <div class="main-categori-wrap d-none d-lg-block">
              <div
                class="main-menu main-menu-padding-1 main-menu-lh-2 d-none d-lg-block"
              >
                <nav>
                  <ul>
                    <li><a class="active" href="/">Home</a></li>
                    <li>
                      <a href="/product-list">Shop</a>
                    </li>
                  </ul>
                </nav>
              </div>
            </div>
            <div class="header-action-right d-block d-lg-none">
              <div class="header-action-2">
                <% if(user){ %>
                <div class="header-action-icon-2">
                  <a href="/wishlist">
                    <img
                      alt="Evara"
                      src="customassets/imgs/theme/icons/icon-heart.svg"
                    />
                    <span class="pro-count white"><%= wishlistCount %></span>
                  </a>
                </div>
                <% } else { %>
                <div class="header-action-icon-2">
                  <a href="/login">
                    <img
                      alt="Evara"
                      src="customassets/imgs/theme/icons/icon-heart.svg"
                    />
                    <span class="pro-count white">0</span>
                  </a>
                </div>
                <% } %>
                <div class="header-action-icon-2" id="addToCart">
                  <% if(user){ %>
                  <a class="mini-cart-icon" href="/cart">
                    <img
                      alt="Evara"
                      src="customassets/imgs/theme/icons/icon-cart.svg"
                    />
                    <span class="pro-count white"><%=cartCount%></span>
                  </a>
                  <% } else { %>
                  <a class="mini-cart-icon" href="/cart">
                    <img
                      alt="Evara"
                      src="customassets/imgs/theme/icons/icon-cart.svg"
                    />
                    <span class="pro-count blue">0</span>
                  </a>
                  <% } %>
                </div>
                <div class="header-action-icon-2 d-block d-lg-none">
                  <div class="burger-icon burger-icon-white">
                    <span class="burger-icon-top"></span>
                    <span class="burger-icon-mid"></span>
                    <span class="burger-icon-bottom"></span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </header>
  </body>
</html>
