<script>
    function addToCart(proId) {
      $.ajax({
        url: "/add-to-cart?id=" + proId,
        method: "get",
        success: (response) => {
          console.log(response.cartCount);
          if (response.status) {
            Toastify({
                text: 'Product added to cart !',
                duration: 3000,
                gravity: 'top',
                position: 'center',
                backgroundColor: "linear-gradient(90deg, #00b09b 0%, #96c93d 100%)",
                stopOnFocus: true,
                className: "toastify-icon",
                close: false,
            }).showToast();
            $("#cartCount").html('<span>' + response.cartCount + '</span>');
          }
        },
      });
    }
    function wishList(proId){
        $.ajax({
            url:"/add-to-wishList?id="+ proId,
            method:"get",
            success:(response)=>{
                console.log(response)
                if(response.status){
                    Toastify({
                        text: 'Product added to WishList',
                        duration: 3000,
                        gravity: 'top',
                        position: 'center',
                        backgroundColor: "linear-gradient(90deg, #00b09b 0%, #96c93d 100%)",
                        stopOnFocus: true,
                        className: "toastify-icon",
                        close: false,
                    }).showToast();
                    $("#wishCount").html('<span>' + response.wishlistCount + '</span>');
                }else{
                    Toastify({
                        text: 'Product removed from WishList',
                        duration: 3000,
                        gravity: 'top',
                        position: 'center',
                        backgroundColor: "linear-gradient(90deg, #00b09b 0%, #96c93d 100%)",
                        stopOnFocus: true,
                        className: "toastify-icon",
                        close: false,
                    }).showToast();
                    $("#wishCount").html('<span>' + response.wishlistCount + '</span>');
                }
            }
        })
    }
</script>
